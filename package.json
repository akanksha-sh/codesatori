{
  "name": "codesatori_frontend",
  "version": "0.1.0",
  "engines": {
    "npm": "6.x"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "bootstrap-css-only": "^4.4.1",
    "firebase": "^7.15.0",
    "material-design-icons": "^3.0.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-anime": "^3.0.3",
    "react-dom": "^16.13.1",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "reactstrap": "^8.4.1",
    "serve": "11.3.1",
    "table": "^5.4.6"
  },
  "scripts": {
    "dev": "react-scripts start",
    "envdev": "export $(cat .env | xargs) && react-scripts start",
    "start": "serve -s build -l $PORT || serve -s build -l 5000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm update && npm install && npm run build",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "env": "export $(cat .env | xargs) && env"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
